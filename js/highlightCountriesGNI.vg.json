{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
    "title": "GNI of Highlighted Countries",
    "height": 600,
    "width": 800,
  
    "data": {"url": "https://raw.githubusercontent.com/jfan0010/FIT3179Week10Homwork/main/data/normalisedGniHighlightedCountries.csv"},
